<Window x:Class="FriendsClientWpf.AddFriendDialog"
        x:Name="AddFriendWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FriendsClientWpf"
        mc:Ignorable="d"
        Title="AddFriendDialog" Height="241.5" Width="312.5" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <local:FriendInfo x:Name="friendInfo" FriendContext="{Binding FriendContext,ElementName=AddFriendWindow}">
        </local:FriendInfo>
        <Button x:Name="s" Content="Add Friend" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="76" Click="s_Click"/>
    </Grid>
</Window>
